<DataScreen>:
# Screen:

    MDBoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: "Data"
            type: "bottom"
            specific_text_color: gch("#060606")
            left_action_items: [["keyboard-backspace", lambda x: root.back()]]
            right_action_items: [["refresh", lambda x: root.view()]]
        MDBoxLayout
            orientation: 'vertical'
            MDLabel:
                text: 'Data Barang'  
                font_style: 'H5'
                bold: True
                halign: 'center'
                size_hint_y: .1
            MDBoxLayout
                orientation:"vertical"
                padding: [dp(10), dp(10), dp(10), dp(10)]
                id: table_id
